{
  "name": "@polareth/evmstate",
  "version": "0.0.0-semantically-released",
  "description": "A TypeScript library for tracing, and visualizing EVM state changes with detailed human-readable labeling.",
  "keywords": [
    "evm",
    "state",
    "transaction",
    "analysis",
    "storage",
    "layout",
    "slot",
    "semantic",
    "labeling",
    "trace"
  ],
  "author": "0xpolarzero",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/polareth/evmstate.git"
  },
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
    "prepare": "tsx scripts/prepare.ts",
    "docs:dev": "pnpm build && vocs dev",
    "docs:build": "pnpm build && vocs build",
    "docs:preview": "pnpm build && vocs preview",
    "generate:layouts": "DEBUG=trace tsx test/generate.ts",
    "test": "pnpm test:unit && pnpm test:staging",
    "test:unit": "DEBUG=trace dotenvx run -f .env.test --quiet -- vitest test/unit",
    "test:staging": "DEBUG=trace TEST_ENV=staging dotenvx run -f .env.test --quiet -- vitest test/staging"
  },
  "dependencies": {
    "@shazow/whatsabi": "^0.21.0",
    "abitype": "^1.0.8",
    "debug": "^4.4.0",
    "tevm": "1.0.0-next.143",
    "viem": "^2.21.1"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@testing-library/react": "^16.3.0",
    "@tevm/bundler-cache": "1.0.0-next.134",
    "@tevm/compiler": "1.0.0-next.134",
    "@tevm/ts-plugin": "1.0.0-next.131",
    "@types/debug": "^4.1.12",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.507.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "tsc-alias": "^1.8.15",
    "tsx": "^3.14.0",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9",
    "vocs": "1.0.11"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tevm": "1.0.0-next.143"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
